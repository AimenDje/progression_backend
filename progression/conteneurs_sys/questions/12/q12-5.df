FROM system

RUN mkdir /home/krusty/inclassables
COPY fichiers/* /home/krusty/inclassables/
RUN mkdir /home/krusty/KrustY
RUN mv /home/krusty/inclassables/*k*y* /home/krusty/KrustY
RUN mkdir /home/krusty/tri
RUN mv /home/krusty/inclassables/??? /home/krusty/tri
RUN chown krusty:krusty /home/krusty/inclassables -R
RUN chown krusty:krusty /home/krusty/KrustY -R
RUN chown krusty:krusty /home/krusty/tri -R


